{
    "name": "workspace-scripts",
    "type": "module",
    "version": "0.0.1",
    "license": "MIT",
    "homepage": "https://github.com/xiaohaiH/vite-vue-visual-cli/tree/master/scripts",
    "repository": {
        "directory": "packages/scripts",
        "type": "git",
        "url": "git+https://github.com/xiaohaiH/vite-vue-visual-cli/tree/master/scripts"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "**.d.ts",
        "dist"
    ],
    "scripts": {
        "build": "tsup --clean",
        "prepare:type": "tsup --dts-only",
        "stub": "tsup --watch --onSuccess \"tsup --dts-only\""
    },
    "dependencies": {
        "birpc": "^2.3.0",
        "hookable": "^5.5.3",
        "mitt": "catalog:",
        "speakingurl": "^14.0.1",
        "superjson": "^2.2.2"
    },
    "devDependencies": {
        "@types/speakingurl": "^13.0.6",
        "inquirer": "^12.6.1",
        "vue": "catalog:",
        "vue-router": "catalog:"
    }
}
